<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Button text="Count" ui:field="countButton"/>
		<g:Button ui:field="persistButton" text="Persist" />
		<g:Button text="Persist Invalid" ui:field="persistInvalidButton"/>
		<g:Button text="List" ui:field="listButton"/>
		<g:Button text="DeleteAll" ui:field="deleteAllButton"/>
		<g:HorizontalPanel>
			<g:TextBox ui:field="inputText"/>
			<g:Button ui:field="fetchButton" text="Fetch"/>
		<g:Button ui:field="updateButton" text="Update"/>
		<g:Button ui:field="deleteButton" text="Delete"/>
		</g:HorizontalPanel>

	</g:HTMLPanel>
</ui:UiBinder> 