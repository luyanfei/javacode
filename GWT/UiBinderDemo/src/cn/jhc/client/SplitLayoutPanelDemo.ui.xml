<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.container {
			border: 3px solid #EEEEEE;
		}
	</ui:style>
	<g:SplitLayoutPanel addStyleNames="{style.container}">
		<g:north size="100.0">
			<g:HTML wordWrap="true">North</g:HTML>
		</g:north>
		<g:west size="100.0">
			<g:HTML wordWrap="true">West</g:HTML>
		</g:west>
		<g:center>
			<g:HTML wordWrap="true">Center</g:HTML>
		</g:center>
		<g:east size="100.0">
			<g:HTML wordWrap="true">East</g:HTML>
		</g:east>
	</g:SplitLayoutPanel>
</ui:UiBinder> 